  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6272a4",
        "fontStyle": "italic"
      }
    },
    {
      "name": "envKeys",
      "scope": "string.quoted.single.ini,constant.numeric.ini,string.quoted.double.ini",
      "settings": {
        "foreground": "#79ddf0",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#ff79c6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#ffffb8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#50fa7b",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#e0e878",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
